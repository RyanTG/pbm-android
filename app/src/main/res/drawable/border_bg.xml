<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent"/>
        </shape>
    </item>
    <item
        android:top="5dp"
        android:left="5dp"
        android:right="5dp"
        android:bottom="5dp">
        <shape>
            <solid
                android:color="#FFFFFF"/>
            <corners
                android:radius="0dp" />
            <padding
                android:left="0dp"
                android:top="0dp"
                android:right="0dp"
                android:bottom="0dp" />
            <stroke
                android:width="5dp"
                android:color="#d3ecff" />
        </shape>
    </item>
</layer-list>